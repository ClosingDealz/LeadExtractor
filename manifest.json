{
    "manifest_version": 3,
    "name": "Lead Extractor",
    "description": "A tool used for extracting leads",
    "version": "1.0.0",
    "action": {
      "default_popup": "index.html"
    },
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["contentScript.js"]
      }
    ]
  }